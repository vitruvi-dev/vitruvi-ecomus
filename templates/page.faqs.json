    "accordion_inline_TYMAQf": {
      "type": "accordion_inline",
      "blocks": {
        "accordion_item_B8X6fi": {
          "type": "accordion_item",
          "settings": {
            "title": "What are your estimated shipping times?",
            "content": "Standard shipping takes approximately 3-5 business days. Expedited shipping takes approximately 2-3 business days."
          }
        },
        "accordion_item_XPMc3R": {
          "type": "accordion_item",
          "settings": {
            "title": "Do you offer expedited shipping?",
            "content": "Yes! We do offer expedited shipping (2-3 business days) across the United States and Canada. Please note this is an additional charge, and will be charged at checkout."
          }
        },
        "accordion_item_hYVatx": {
          "type": "accordion_item",
          "settings": {
            "title": "How long are your processing times?",
            "content": "We work as quickly as possible to get your package shipped out to you in a few days. However, during peak seasons, your order may take up to 4 business days to process."
          }
        },
        "accordion_item_rbC9e3": {
          "type": "accordion_item",
          "settings": {
            "title": "How much is shipping?",
            "content": "Orders over $75 (before tax) ship free, with standard shipping, to Canada and the United States. Orders to Puerto Rico, Hawaii, and Alaska have a $30 shipping charge; this fee is waived on orders over $200."
          }
        }
      },
      "block_order": [
        "accordion_item_B8X6fi",
        "accordion_item_XPMc3R",
