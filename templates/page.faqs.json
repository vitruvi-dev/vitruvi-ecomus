        "accordion_item_6QrWbQ": {
          "type": "accordion_item",
          "settings": {
            "title": "Do your devices come with a warranty?",
            "content": "We offer a lifetime warranty on all of our devices; this includes diffusers and humidifiers. If your device has any mechanical or manufacturing defects, we would be happy to assist you.\n\nNote: Stone Diffusers with a DC power cord are not covered under warranty. For more information, visit our warranty page."
          }
        },
        "accordion_item_KeNUnd": {
          "type": "accordion_item",
          "settings": {
            "title": "How do I claim a product under warranty?",
            "content": "If your registered device stops working, send us the email you used to register your product, information on the issue you’re experiencing (please include a photo or video), and your current shipping address to us at hello@vitruvi.com. We’ll be happy to help you out."
          }
