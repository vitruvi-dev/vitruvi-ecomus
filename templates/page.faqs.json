      "type": "accordion_inline",
      "blocks": {
        "accordion_item_nzJ4yd": {
          "type": "accordion_item",
          "settings": {
            "title": "Are promotional codes eligible during sales and on sale items?",
            "content": "No - no other promotions are applicable during sales, or on any discounted items."
          }
        },
        "accordion_item_atPg3C": {
          "type": "accordion_item",
          "settings": {
            "title": "What is the return and exchange policy for products purchased on sale?",
            "content": "All sale and discounted items are only available for exchange or return for store credit within 15 days of purchase."
          }
        },
        "accordion_item_A7qqR3": {
          "type": "accordion_item",
          "settings": {
            "title": "What is your price adjustment policy?",
            "content": "If you have purchased an item up to 30 days before a sale, we would be happy to offer you a price adjustment during the sale period. The reimbursement will be applied back onto your original form of payment. Please note that the item will be final sale after the price has been adjusted."
          }
        },
        "accordion_item_BgkagJ": {
          "type": "accordion_item",
          "settings": {
            "title": "Can I redeem by Take Care Club points on discounted and sale items?",
            "content": "Yes! To redeem your points, sign into your Take Care Club account through the rewards panel on the bottom left corner of your screen. Select “Ways To Redeem” to view all of your available rewards. To use your points, click “Redeem”."
          }
        }
